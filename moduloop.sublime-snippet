	<content><![CDATA[
//modulo op

ll add(ll a,ll b){
    return (a+b)%mod;
}

ll sub(ll a,ll b){
    return ((a-b)%mod+mod)%mod;
}

ll mul(ll a,ll b){
    return (a*b)%mod;
}

//end of modulo op
]]></content>
